<application>

  <name>Tutorial on Joint Interface System</name>

  <module>
      <name>yarplogger</name>
      <parameters>--start</parameters>
      <node>localhost</node>
  </module>

  <module>
      <name>gzserver</name>
      <parameters>tutorial_joint-interface.sdf</parameters>
      <dependencies>
         <port timeout="20">/yarplogger</port>
      </dependencies>
      <environment>YARP_FORWARD_LOG_ENABLE=1</environment>
      <node>localhost</node>
  </module>

  <module>
      <name>gzclient</name>
      <node>localhost</node>
  </module>

</application>
